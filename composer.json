{
    "name": "jms/twig-js-bundle",
    "type": "symfony-bundle",
    "description": "Integrates twig.js into your Symfony2 application",
    "keywords": ["templating"],
    "homepage": "http://jmsyst.com/bundles/JMSTwigJsBundle",
    "license": "Apache2",
    "authors": [
        {
            "name": "Johannes M. Schmitt",
            "email": "schmittjoh@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "jms/twig-js": "~1.0",
        "symfony/framework-bundle": "~2.1|~3.0",
        "symfony/twig-bridge": "~2.1|~3.0",
        "twig/twig": "~1.0,<1.25.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "autoload": {
        "psr-0": { "JMS\\TwigJsBundle": "" }
    },
    "target-dir": "JMS/TwigJsBundle"
}
